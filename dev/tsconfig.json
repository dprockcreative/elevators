{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "lib": [ "es2015", "dom" ]
  },
  "exclude": [
    "./node_modules/*",
    "./aot/*",
    "./app/aot.*"
  ],
  "typeRoots": [ "./node_modules/@types/jasmine", "./node_modules/@types/node" ],
  "types": []
}
